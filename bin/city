#!/usr/bin/env python

from sys import argv
from importlib import import_module

_, city, *args = argv

try:
    city_class = getattr(import_module('invisible_cities.cities.'+city), city.capitalize())
except ModuleNotFoundError:
    print('available cities: <TODO look at contents of config directory>')
    print('usage TODO: <get this from our config parser>')
else:
    city_class.drive(['city {city}'.format(**locals())] + args)
